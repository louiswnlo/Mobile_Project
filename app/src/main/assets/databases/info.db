CREATE TABLE service (
service_id varchar(10) PRIMARY KEY,
name varchar,
discount smallint);
CREATE TABLE payment(
payment_id varchar(10) PRIMARY KEY,
user_id varchar(10),
data DATA,
amount smallint,
status_code char(1),
FOREIGN KEY (user_id) REFERENCES user(user_id)
);
CREATE TABLE review(
review_id varchar(20) PRIMARY KEY,
user_id varchar (10),
title varchar,
comment varchar,
rating char(1),
photo_url image,
FOREIGN KEY (user_id) references user(user_id)
);
CREATE TABLE shop(
shop_id varchar(10) PRIMARY KEY,
service_id varchar(10),
shop_name varchar,
location varchar,
discount smallint,
logo_url image,
photo_url image,
district_id varchar(2),
shop_info varchar,
FOREIGN KEY (service_id) references service(service_id)
);
