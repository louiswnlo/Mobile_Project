PRAGMA foreign_keys=OFF;
BEGIN TRANSACTION;
CREATE TABLE user(
user_id varchar(10) PRIMARY KEY,
gender char(1),
username varchar(10),
password varchar(10),
email varchar,
phone varchar (8),
first_name varchar,
last_name varchar,
display_name varchar
);
